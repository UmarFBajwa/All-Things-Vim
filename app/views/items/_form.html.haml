= form_for :items, :url => items_path do |f|
  %p
    = f.label :name
    = f.text_field :name

  %p
    = f.label :price
    = f.text_field :price

  %p
    = f.label :description
    = f.text_field :description

  %p
    = f.label :image_url
    = f.text_field :image_url

  %p
    = f.submit(type = "Submit")

